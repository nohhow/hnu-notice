{
  "manifest_version": 2,

  "name": "HNU-Notice",
  "description": "한남대학교 학사, 취업, 장학, 컴퓨터공학과 공지를 한 번에 확인",
  "version": "1.0.5",
  "author" : "Jinhyun Noh",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "permissions": [
    "<all_urls>",
    "topSites"
  ],

  "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net;     object-src 'self'",


  "chrome_url_overrides" : {
    "newtab" : "index.html" 
  }
}